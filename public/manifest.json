{
    "name": "BookmarkSync",
    "description": "browser extension to perform bookmark synchronization accross browsers.",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {},
    "background": {
        "service_worker": "index.js"
    },
    "icons": {
        "64": "reload.svg",
        "128": "reload.svg",
        "256": "reload.svg"
    }
}